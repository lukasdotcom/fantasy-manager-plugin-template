{
  "name": "fantasy-manager-plugin-template",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "lint": "eslint . --max-warnings 0",
    "pretty": "prettier --check .",
    "format": "prettier --write .",
    "test": "tsc -p tsconfig.json && npm run lint && npm run pretty",
    "start": "ts-node --project=tsconfig.json ./main.ts"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^8.35.0",
    "@typescript-eslint/parser": "^8.35.0",
    "eslint": "^9.29.0",
    "prettier": "^3.5.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  },
  "imports": {
    "#/*": "./*.ts",
    "#types/*": "./types/*.ts"
  }
}
